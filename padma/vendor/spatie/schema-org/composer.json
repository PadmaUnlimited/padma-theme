{
    "name": "spatie/schema-org",
    "description": "A fluent builder Schema.org types and ld+json generator",
    "keywords": [
        "spatie",
        "schema-org"
    ],
    "homepage": "https://github.com/spatie/schema-org",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Tom Witkowski",
            "email": "dev.gummibeer@gmail.com",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0.4",
        "illuminate/collections": "^8.16.1",
        "league/flysystem": "^1.1.3",
        "phpunit/phpunit": "^9.4.3",
        "symfony/console": "^3.4.47",
        "twig/twig": "^1.44.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SchemaOrg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SchemaOrg\\Generator\\": "generator",
            "Spatie\\SchemaOrg\\Test\\": "tests"
        }
    },
    "scripts": {
        "generate": "php ./generate.php",
        "test": "vendor/bin/phpunit"
    }
}
